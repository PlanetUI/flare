		"@sveltejs/kit": "1.0.11",
		"@typescript-eslint/eslint-plugin": "5.48.1",
		"@typescript-eslint/parser": "5.48.1",
		"autoprefixer": "10.4.13",
		"eslint": "8.31.0",
		"eslint-config-prettier": "8.6.0",
		"eslint-plugin-svelte3": "4.0.0",
		"mongodb": "4.13.0",
		"postcss": "8.4.21",
		"postgres": "3.3.3",
		"prettier": "2.8.2",
		"prettier-plugin-svelte": "2.9.0",
		"prisma": "4.8.1",
		"svelte": "3.55.1",
		"svelte-check": "3.0.2",
		"tailwindcss": "3.2.4",
		"tslib": "2.4.1",
